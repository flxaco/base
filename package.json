{
  "name": "root",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-jest": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "browserslist": "^4.14.3",
    "copyfiles": "^2.3.0",
    "css-loader": "^4.3.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "http-server": "^0.12.3",
    "jest": "^26.4.2",
    "lerna": "^3.22.1",
    "mini-css-extract-plugin": "^0.11.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "postcss-color-function": "^4.1.0",
    "postcss-css-variables": "^0.13.0",
    "postcss-import": "^12.0.1",
    "postcss-inline-svg": "^4.1.0",
    "postcss-loader": "^4.0.2",
    "postcss-mixins": "^6.2.3",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-preset-env": "^6.7.0",
    "react-is": "^16.13.1",
    "style-loader": "^1.2.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-formatter-pretty": "^1.1.4",
    "svg-react-loader": "^0.4.6",
    "wait-on": "^3.3.0"
  },
  "scripts": {
    "start": "npm run watch",
    "build": "npm-run-all clean-packages lerna:bootstrap lerna:build",
    "watch": "npm run storybook",
    "lerna:bootstrap": "lerna bootstrap --ignore-scripts --no-ci --hoist",
    "lerna:build": "lerna run prepare",
    "lerna:publish": "lerna version && lerna publish",
    "lint:css": "stylelint packages/*/src/**/*.css --custom-formatter=node_modules/stylelint-formatter-pretty",
    "lint:js": "eslint packages/*/src/**/*.js scripts/**/*.js -f pretty",
    "lint": "npm-run-all lint:*",
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "create-package": "./scripts/create-package.sh",
    "link-packages": "./scripts/link-packages.sh",
    "clean-packages": "./scripts/clean-packages.sh",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "npm run clean-storybook-static && build-storybook",
    "serve-storybook-static": "http-server ./storybook-static",
    "clean-storybook-static": "rm -rf ./storybook-static"
  },
  "browserslist": "last 2 versions, > 0.25%, firefox esr, ie 11, not dead",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "jest-css-modules-processor": "0.0.9",
    "jest-transform-css": "^2.0.0",
    "object-fit-images": "^3.2.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-test-renderer": "^16.13.1"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "jestCSSProcessor": {
    "generateScopedName": "[name]__[local]___[hash:base64:5]"
  }
}
