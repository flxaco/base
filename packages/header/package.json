{
  "name": "@flxaco/header",
  "version": "0.0.3",
  "description": "header component",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flxaco/base/tree/master/packages/header"
  },
  "author": "Nick Fletcher <hi@flxa.co>",
  "license": "MIT",
  "style": "src/header.css",
  "files": [
    "dist/*",
    "src/*"
  ],
  "eyeglass": {
    "name": "flxaco-header",
