{
  "name": "@pnx-mixtape/tiles",
  "version": "0.0.4",
  "description": "tiles component",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/previousnext/mixtape/tree/master/packages/tiles"
  },
  "author": "PreviousNext <info@previousnext.com.au>",
